package com.iwonder.tea.prd.entity;

import java.io.Serializable;
import java.util.Date;

/**
 * 实体类：商品
 * 
 * @author Tearell
 *
 */
public class Product implements Serializable {
	/**	 */
	private static final long serialVersionUID = 1L;

	/** 产品id */
	private String productId;

	/** 小分类id */
	private String typeDtlId;

	/** 品牌id */
	private String brandId;

	/** 产品名称 */
	private String name;

	/** 上市时间 */
	private Date timeToMarket;

	/** 介绍 */
	private String introduce;

	/** 上架状态 */
	private String shelfStat;

	/** 好评数 */
	private Integer evalGoodQty;

	/** 中评数 */
	private Integer evalGeneralQty;

	/** 差评数 */
	private Integer evalBadQty;

	/** 销量 */
	private Integer saleQty;

	/** 产品图地址 */
	private String picture;

	private String majorSkuCode;

	/**
	 * 获取：产品id
	 * 
	 * @return the productId
	 */
	public String getProductId() {
		return productId;
	}

	/**
	 * 设置：产品id
	 * 
	 * @param productId
	 *            the productId to set
	 */
	public void setProductId(String productId) {
		this.productId = productId;
	}

	/**
	 * 获取：小分类id
	 * 
	 * @return the typeDtlId
	 */
	public String getTypeDtlId() {
		return typeDtlId;
	}

	/**
	 * 设置：小分类id
	 * 
	 * @param typeDtlId
	 *            the typeDtlId to set
	 */
	public void setTypeDtlId(String typeDtlId) {
		this.typeDtlId = typeDtlId;
	}

	/**
	 * 获取：品牌id
	 * 
	 * @return the brandId
	 */
	public String getBrandId() {
		return brandId;
	}

	/**
	 * 设置：品牌id
	 * 
	 * @param brandId
	 *            the brandId to set
	 */
	public void setBrandId(String brandId) {
		this.brandId = brandId;
	}

	/**
	 * 获取：产品名称
	 * 
	 * @return the name
	 */
	public String getName() {
		return name;
	}

	/**
	 * 设置：产品名称
	 * 
	 * @param name
	 *            the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}

	/**
	 * 获取：上市时间
	 * 
	 * @return the timeToMarket
	 */
	public Date getTimeToMarket() {
		return timeToMarket;
	}

	/**
	 * 设置：上市时间
	 * 
	 * @param timeToMarket
	 *            the timeToMarket to set
	 */
	public void setTimeToMarket(Date timeToMarket) {
		this.timeToMarket = timeToMarket;
	}

	/**
	 * 获取：产品介绍
	 * 
	 * @return the introduce
	 */
	public String getIntroduce() {
		return introduce;
	}

	/**
	 * 设置：产品介绍
	 * 
	 * @param introduce
	 *            the introduce to set
	 */
	public void setIntroduce(String introduce) {
		this.introduce = introduce;
	}

	/**
	 * 获取：上架状态
	 * 
	 * @return the shelfStat
	 */
	public String getShelfStat() {
		return shelfStat;
	}

	/**
	 * 设置：上架状态
	 * 
	 * @param shelfStat
	 *            the shelfStat to set
	 */
	public void setShelfStat(String shelfStat) {
		this.shelfStat = shelfStat;
	}

	/**
	 * 获取：好评数量
	 * 
	 * @return the evalGoodQty
	 */
	public Integer getEvalGoodQty() {
		return evalGoodQty;
	}

	/**
	 * 设置：好评数量
	 * 
	 * @param evalGoodQty
	 *            the evalGoodQty to set
	 */
	public void setEvalGoodQty(Integer evalGoodQty) {
		this.evalGoodQty = evalGoodQty;
	}

	/**
	 * 获取：中评数量
	 * 
	 * @return the evalGeneralQty
	 */
	public Integer getEvalGeneralQty() {
		return evalGeneralQty;
	}

	/**
	 * 设置：中评数量
	 * 
	 * @param evalGeneralQty
	 *            the evalGeneralQty to set
	 */
	public void setEvalGeneralQty(Integer evalGeneralQty) {
		this.evalGeneralQty = evalGeneralQty;
	}

	/**
	 * 获取：差评数量
	 * 
	 * @return the evalBadQty
	 */
	public Integer getEvalBadQty() {
		return evalBadQty;
	}

	/**
	 * 设置：差评数量
	 * 
	 * @param evalBadQty
	 *            the evalBadQty to set
	 */
	public void setEvalBadQty(Integer evalBadQty) {
		this.evalBadQty = evalBadQty;
	}

	/**
	 * 获取：销量
	 * 
	 * @return the saleQty
	 */
	public Integer getSaleQty() {
		return saleQty;
	}

	/**
	 * 设置：销量
	 * 
	 * @param saleQty
	 *            the saleQty to set
	 */
	public void setSaleQty(Integer saleQty) {
		this.saleQty = saleQty;
	}

	/**
	 * 获取：产品图地址
	 * 
	 * @return the picture
	 */
	public String getPicture() {
		return picture;
	}

	/**
	 * 设置：产品图地址
	 * 
	 * @param picture
	 *            the picture to set
	 */
	public void setPicture(String picture) {
		this.picture = picture;
	}

	public String getMajorSkuCode() {
		return majorSkuCode;
	}

	public void setMajorSkuCode(String majorSkuCode) {
		this.majorSkuCode = majorSkuCode;
	}

	@Override
	public String toString() {
		return "Product [productId=" + productId + ", typeDtlId=" + typeDtlId + ", brandId=" + brandId + ", name="
				+ name + ", timeToMarket=" + timeToMarket + ", introduce=" + introduce + ", shelfStat=" + shelfStat
				+ ", evalGoodQty=" + evalGoodQty + ", evalGeneralQty=" + evalGeneralQty + ", evalBadQty=" + evalBadQty
				+ ", saleQty=" + saleQty + ", picture=" + picture + ", majorSkuCode=" + majorSkuCode + "]";
	}
}
