.all{
	width: 1903px;
	height: 900px;
}
a{
	color: #666666;
}
.all>img{
	position: absolute;
	margin-left: 232px;
}

/********************header********************************/
.header{
	width: 990px;
	height: 50px;
	margin: 0 auto;
}
.header>ul{
	float: right;
}
.header>img{
	top: 15px;
}
.header ul li{
	float: left;
	margin-right: 15px;
	margin-top: 15px;
	color: #DDDDDD;
}

