package com.iwonder.tea.prd.view;

import java.io.Serializable;

import com.iwonder.tea.prd.entity.ProductSku;

public class ProductSkuQueryView extends ProductSku implements Serializable {

	private static final long serialVersionUID = 1L;

	private String productName;

	private String colorValue;

	private String sizeValue;
	
	public String getProductName() {
		return productName;
	}

	public void setProductName(String productName) {
		this.productName = productName;
	}

	public String getColorValue() {
		return colorValue;
	}

	public void setColorValue(String colorValue) {
		this.colorValue = colorValue;
	}

	public String getSizeValue() {
		return sizeValue;
	}

	public void setSizeValue(String sizeValue) {
		this.sizeValue = sizeValue;
	}

	@Override
	public String toString() {
		return "ProductSkuQueryView [productName=" + productName + ", colorValue=" + colorValue + ", sizeValue="
				+ sizeValue + "]";
	}

}
