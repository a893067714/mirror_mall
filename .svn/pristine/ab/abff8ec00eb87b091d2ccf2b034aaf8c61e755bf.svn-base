*{
	margin: 0;
	padding: 0;
	position: relative;
	font: tahoma,arial,Microsoft YaHei,Hiragino Sans GB,"\u5b8b\u4f53",sans-serif;
	font-size: 12px;
}
.clearfix:after{
	content:'.';
	display:block;
	clear:both;
	visibility:hidden;
	height: 0px;
}
ul{
	list-style: none;
}
a{
	text-decoration: none;
	color: #999999;
}

span{
	display: inline-block;
}
img {
	
}

